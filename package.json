{
  "name": "guidelines-config-tool",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:remote:be": "ng serve --disableHostCheck=true --configuration production --port 4700 --ssl=true",
    "build": "ng build --prod --configuration production",
    "postbuild": "npm run manifest:update",
    "test": "ng test",
    "test-coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-push-dev": "ng build --prod --configuration dev && aws s3 cp --recursive dist/guidelinesConfigTool/ s3://cdsgctdev-astack-cds-guidelines-config-tool/",
    "build-push-test": "ng build --prod --configuration test && aws s3 cp --recursive dist/guidelinesConfigTool/ s3://cdsgctqa-astack-cds-guidelines-config-tool-ui/",
    "build-push-prod": "ng build --prod --configuration production && aws s3 cp --recursive dist/guidelinesConfigTool/ s3://cdsgct-astack-cds-guidelines-config-tool/",
    "manifest:update": "ts-node -P tools/tsconfig.tools.json tools/scripts/manifest.ts guidelinesConfigTool"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/elements": "^10.0.14",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.11",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/entity": "^10.0.1",
    "@ngrx/schematics": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "@tinymce/tinymce-angular": "^4.2.0",
    "d3": "^6.2.0",
    "document-register-element": "^1.7.2",
    "lodash": "^4.17.20",
    "panzoom": "^9.4.1",
    "ramda": "^0.27.1",
    "rxjs": "~6.5.5",
    "tinymce": "^5.5.1",
    "tslib": "^2.0.0",
    "whatwg-fetch": "^3.6.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular/cli": "~10.0.7",
    "@angular/compiler-cli": "~10.0.11",
    "@ngneat/spectator": "^6.1.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.65",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^11.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
